[GENERAL] 
#startFrom = "restart.fld"
polynomialOrder = 7
numSteps = 1000 
dt = 2e-02
timeStepper = tombo2
checkpointInterval = 100

[PRESSURE]
residualTol = 1e-06

[VELOCITY]
boundaryTypeMap = codedFixedValue, zeroGradient, zeroValue 
residualTol = 1e-08

[TEMPERATURE]
boundaryTypeMap = codedFixedValue, zeroGradient, zeroGradient 
residualTol = 1e-08
